def myfunc(a):
    print("inside function")
    print("value revieved in a as",a)
    new=[3,5]
    a=new
    a.append(6)
    print("a changes to",a)
    print("Returning from function")
num=[1,4]
print("Calling myfunc() by passing 'num' as ",num)
myfunc(num)
print("After calling myfunc() 'num' as",num)
